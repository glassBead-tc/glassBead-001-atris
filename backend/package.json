{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "type": "module",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --declaration --outDir dist/",
    "start": "tsx --experimental-wasm-modules -r dotenv/config app/index.ts dotenv_config_path=.env",
    "start:dist": "yarn build && node -r dotenv/config dist/index.js",
    "lint": "eslint --cache --ext .ts,.js src/",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged",
    "format": "prettier --write \"src/**/*.ts\" \"src/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/*.ts\"",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --config jest.config.cjs",
    "interactive": "tsx app/interactiveTest.ts"
  },
  "author": "Brace Sproul",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.2.9",
    "@langchain/core": "^0.2.6",
    "@langchain/langgraph": "^0.0.22",
    "@langchain/openai": "^0.1.2",
    "axios": "^1.7.7",
    "compromise": "^14.14.2",
    "dotenv": "^16.4.5",
    "langchain": "^0.2.5",
    "node-nlp": "^4.27.0",
    "readline": "^1.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.25.7",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.3",
    "ts-jest": "29.2.5",
    "tsx": "^3.12.3",
    "typescript": "^5.0.0"
  }
}

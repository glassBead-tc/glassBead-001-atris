{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/app/index.js",
  "type": "module",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc && cp -r src/app/data dist/app/data",
    "start": "node --experimental-wasm-modules -r dotenv/config dist/app/index.js dotenv_config_path=.env",
    "start:dist": "npm run build && node -r dotenv/config dist/app/index.js",
    "lint": "eslint --cache --ext .ts,.js src/",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "format": "prettier --write \"src/**/*.ts\" \"src/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/*.ts\"",
    "test": "node --experimental-vm-modules $(npm bin)/jest --config jest.config.cjs",
    "interactive": "node app/interactiveTest.js",
    "dev": "tsc --watch"
  },
  "author": "Brace Sproul",
  "license": "MIT",
  "dependencies": {
    "@audius/sdk": "^6.0.1",
    "@babel/core": "^7.25.8",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@langchain/community": "^0.3.5",
    "@langchain/core": "^0.3.13",
    "@langchain/langgraph": "^0.2.16",
    "@langchain/openai": "^0.3.7",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/web3.js": "^1.3.0",
    "axgen": "^0.0.23",
    "axios": "^1.7.7",
    "compromise": "^14.14.2",
    "dotenv": "^16.4.5",
    "eventemitter3": "^5.0.1",
    "express": "^4.17.1",
    "google-protobuf": "^3.21.0",
    "node-nlp": "^4.27.0",
    "pg": "^8.11.0",
    "pg-query-stream": "^4.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "readline": "^1.3.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.6.3",
    "web3": "^1.8.2",
    "webpack": "^5.95.0",
    "winston": "^3.15.0",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "babel-jest": "^29.2.5",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.3",
    "ts-jest": "29.2.5",
    "tsx": "^3.12.3",
    "typescript": "^4.0.0"
  }
}

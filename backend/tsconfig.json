{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "node",
    "baseUrl": "./",
       "paths": {
         "*": ["node_modules/*"]
       },
       "typeRoots": ["node_modules/@types", "node_modules/@audius/sdk/dist/sdk/types"],
    "esModuleInterop": true,
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "rootDir": "src",
    "types": ["node", "jest"],
    "resolveJsonModule": true
  },
  "include": ["src/**/*", "global.d.ts"],
  "exclude": [
    "node_modules/",
    "dist/",
    "__tests__/"
  ],
}
